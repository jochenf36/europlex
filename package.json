{
  "name": "europlex",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start-server": "nodemon server.js",
    "build:js": "browserify assets/js/main.js > dist/main.js",
    "build:css": "stylus assets/styles --out dist/css/",
    "build:html": "jade assets/views/index.jade --out dist/",
    "build": "npm run build:js && npm run build:css && npm run build:html",
    "build:watch": "watch 'npm run build' ./assets",
    "eslint": "eslint 'assets/js/*.js'",
    "browsersync": "browser-sync start --files './assets/**' --proxy localhost:3002 ",
    "dev": "npm run start-server",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jochenf36/europlex.git"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "contentful": "^3.3.5",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "i18n-2": "^0.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.6.1",
    "nib": "^1.1.0",
    "serve-favicon": "~2.3.0",
    "stylus": "^0.53.0"
  },
  "devDependencies": {
    "browser-sync": "latest",
    "browserify": "latest",
    "eslint": "latest",
    "eslint-config-airbnb": "^4.0.0",
    "eslint-plugin-react": "^3.15.0",
    "jade": "latest",
    "nodemon": "^1.8.1",
    "stylus": "latest",
    "watch": "latest"
  }
}
